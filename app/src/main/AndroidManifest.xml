<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.fubei">

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="design_width"
            android:value="640"></meta-data>
        <meta-data
            android:name="design_height"
            android:value="1175"></meta-data>

        <activity
            android:name="com.example.fubei.activity.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.QRCodeActivity"/>

        <activity android:name=".activity.BankCardListActivity"/>

        <activity android:name="com.zbar.lib.CaptureActivity"/>

    </application>

</manifest>
